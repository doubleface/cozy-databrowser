  "description": "Browse all the data in your Cozy",
  "license": "MIT",
  "cozy-permissions": {
    "all": {
      "description": "Databrowser needs access to all your doctypes to display it"
    }
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "backbone": "^1.2.3",
    "cozy-ui": "^0.1.6",
    "css-loader": "^0.26.0",
    "datatables.net": "^1.10.11",
    "datatables.net-buttons": "^1.1.2",
    "datatables.net-buttons-dt": "^1.2.2",
    "datatables.net-dt": "^1.10.12",
    "datatables.net-scroller": "^1.4.1",
    "datatables.net-scroller-dt": "^1.4.2",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "http-server": "^0.9.0",
    "jquery": "^2.2.0",
    "morgan": "^1.7.0",
    "npm-run-all": "^4.0.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "underscore": "^1.8.3",
    "webpack": "^1.13.3"
  },
  "scripts": {
    "serve": "hs ..",
    "start": "run-p watch:* serve",
    "build": "webpack",
    "build:standalone": "webpack --config webpack.standalone.config.js",
    "watch:standalone": "npm run build:standalone -- --watch"
  }
}
